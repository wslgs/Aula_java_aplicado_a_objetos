import java.util.Scanner;

public class Exercicio_03 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int alcool = 0, gasolina = 0, diesel = 0;
		int n = sc.nextInt();
		
		while(n != 4) {
			
			switch (n) {
			case 1:
				alcool++;
				break;
			case 2:
				gasolina++;
				break;
			case 3:
				diesel++;
				break;
			}
			
			n = sc.nextInt();
			
		}
		sc.close();
		
	}

}
